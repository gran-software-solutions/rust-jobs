<html>
<% include!("./head.stpl"); %>

  <body>
    <% include!("./navbar.stpl"); %>

      <h1>Rust job <%= job.title %>
      </h1>

      <fieldset>
        <legend>Job Details:</legend>
        <label for="title">Title:</label>
        <input type="text" name="title" value="<%= job.title %>"><br>
        <label for="title">Type:</label>
        <input type="text" name="type" value="<%= job.job_type | dbg %>"><br>
        <label for="start">Start:</label>
        <input type="text" name="start" value="<%= job.start %>"><br>
        <label for="location">Location:</label>
        <input type="text" name="location" value="<%= job.location | dbg %>"><br>
        <label for="client">Client:</label>
        <input type="text" name="client" value="<%= job.client %>"><br>
      </fieldset>
  </body>

</html>