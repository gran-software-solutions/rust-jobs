<html>
<% include!("./head.stpl"); %>

  <body>
    <% include!("./navbar.stpl"); %>

      <h1>Rust job <%= job.title %>
      </h1>

      <div>
        <fieldset>
          <legend>Job Details:</legend>
          <label for="title">Title:</label>
          <input type="text" name="title" disabled value="<%= job.title %>"><br>
          <label for="title">Type:</label>
          <input type="text" name="type" disabled value="<%= job.job_type | dbg %>"><br>
          <label for="start">Start:</label>
          <input type="text" name="start" disabled value="<%= job.start %>"><br>
          <label for="location">Location:</label>
          <input type="text" name="location" disabled value="<%= job.location | dbg %>"><br>
          <label for="client">Client:</label>
          <input type="text" name="client" disabled value="<%= job.client %>"><br>
        </fieldset>
      </div>
  </body>

</html>