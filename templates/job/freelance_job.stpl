<html>
<% include!("./head.stpl"); %>

  <body>
    <% include!("./navbar.stpl"); %>

      <h1>Rust job <%= job.title %>
      </h1>

      <div>
        <table id="job">
          <tr>
            <td>Title</td>
            <td>
              <%= job.title %>
            </td>
          </tr>
          <tr>
            <td>Type</td>
            <td>
              <%= job.job_type | dbg %>
            </td>
          </tr>
          <tr>
            <td>Start</td>
            <td>
              <%= job.start %>
            </td>
          </tr>
          <tr>
            <td>Location</td>
            <td>
              <%= job.location | dbg %>
            </td>
          </tr>
          <tr>
            <td>Employer</td>
            <td>
              <%= job.employer %>
            </td>
          </tr>
        </table>
      </div>
  </body>

</html>